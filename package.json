{
  "name": "react-minimal-boilerplate",
  "version": "1.0.0",
  "description": "``` Boilerplate for React with babel and webpack ```",
  "dependencies": {
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "style-loader": "^0.13.1",
    "eslint": "^3.0.0",
    "eslint-plugin-react": "^5.2.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "install": "npm i eslint webpack-dev-server -g",
    "lint": "./node_modules/eslint/bin/eslint.js ./src",
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --color --hot --inline --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktrysmt/react-minimal-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "babel"
  ],
  "author": "Kotaro Yoshimatsu <kotaro.yoshimatsu@gmail.com> (https://github.com/ktrysmt)",
  "license": "MIT",
  "homepage": "https://github.com/ktrysmt/react-minimal-boilerplate"
}
